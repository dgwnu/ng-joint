{
  "name": "ng-joint-app",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "dev:lib": "ng build ng-joint --watch",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy:app": "npm run build:app && npm run publish:gh-pages",
    "build:app": "ng build --base-href \"https://dgwnu.github.io/ng-joint/\"",
    "publish:gh-pages": "ngh -d dist/ng-joint-app --repo https://github.com/dgwnu/ng-joint.git --no-silent",
    "build:schematic": "npm run remove:schematic && npm run install:schematic && npm run start:schematic",
    "start:schematic": "ts-node -P projects/ng-joint/src/schematic-build/tsconfig.json projects/ng-joint/src/schematic-build/build.ts",
    "install:schematic": "ng g ng-joint-schematics:build schematic --project=ng-joint",
    "remove:schematic": "npm run remove:schematic:generated && npm run remove:schematic:build",
    "remove:schematic:build": "rimraf projects/ng-joint/src/schematic-build",
    "remove:schematic:generated": "rimraf projects/ng-joint/src/lib/schematic-generated"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.7",
    "@angular/cdk": "~7.3.3",
    "@angular/common": "~7.2.7",
    "@angular/compiler": "~7.2.7",
    "@angular/core": "~7.2.7",
    "@angular/elements": "^7.2.7",
    "@angular/flex-layout": "^7.0.0-beta.23",
    "@angular/forms": "~7.2.7",
    "@angular/material": "~7.3.3",
    "@angular/platform-browser": "~7.2.7",
    "@angular/platform-browser-dynamic": "~7.2.7",
    "@angular/router": "~7.2.7",
    "core-js": "^2.6.5",
    "hammerjs": "^2.0.8",
    "jointjs": "^2.2.1",
    "node-cache": "^4.2.0",
    "rxjs": "~6.4.0",
    "zone.js": "~0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.4",
    "@angular-devkit/build-ng-packagr": "~0.13.4",
    "@angular/cli": "~7.3.4",
    "@angular/compiler-cli": "~7.2.7",
    "@angular/language-service": "~7.2.7",
    "@types/backbone": "^1.3.46",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jquery": "^3.3.29",
    "@types/lodash": "^4.14.122",
    "@types/node": "~10.12.18",
    "angular-cli-ghpages": "^0.5.3",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^4.7.1",
    "protractor": "~5.4.2",
    "rimraf": "^2.6.3",
    "ts-node": "~8.0.2",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.3",
    "tslint": "~5.13.1",
    "typescript": "~3.2.4"
  }
}
