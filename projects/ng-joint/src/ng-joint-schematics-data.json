{
    "$schema": "https://raw.githubusercontent.com/dgwnu/ng-joint-schematics/master/src/ng-joint-schematics/data/ng-joint-schematic-data-schema.json",
    "defaults": {
        "importMappings": [
            {
                "importSymbols": ["shapes", "attributes", "dia"],
                "fromPath": "jointjs"
            }
        ]
    },
    "shapes" : {
        "standard": {
            "defaults": {
                "elements": {
                    "shapeObjectClass": {
                        "nameSpace": "shapes",
                        "class": "standard.__name@classify__"
                    },
                    "shapeOptionsClass": {
                        "nameSpace": "dia",
                        "class": "Element.GenericAttributes",
                        "type": "shapes.standard.__name@classify__Selectors"
                    }
                },
                "links": {
                    "shapeObjectClass": {
                        "nameSpace": "shapes",
                        "class": "standard.__name@classify__"
                    },
                    "shapeOptionsClass": {
                        "nameSpace": "dia",
                        "class": "Link.GenericAttributes",
                        "type": "shapes.standard.__name@classify__Selectors"
                    }
                }
            },
            "elements": {
                "bordered-image": {
                    "properties": {
                        "attrs": {
                            "image": {
                                "nameSpace": "attributes",
                                "class": "SVGImageAttributes"
                            },
                            "border": {
                                "nameSpace": "attributes",
                                "class": "SVGRectAttributes"
                            },
                            "background": {
                                "nameSpace": "attributes",
                                "class": "SVGRectAttributes"
                            },

                            "label": {
                                "nameSpace": "attributes",
                                "class": "SVGTextAttributes"
                            }
                        }
                    }
                },
                "circle": {
                    "properties": {
                        "attrs": {
                            "body": {
                                "nameSpace": "attributes",
                                "class": "SVGPathAttributes"
                            },
                            "label": {
                                "nameSpace": "attributes",
                                "class": "SVGTextAttributes"
                            }
                        }
                    }
                },
                "cylinder": {
                    "properties": {
                        "attrs": {
                            "body": {
                                "nameSpace": "attributes",
                                "class": "SVGPathAttributes"
                            },
                            "top": {
                                "nameSpace": "attributes",
                                "class": "SVGEllipseAttributes"
                            },
                            "label": {
                                "nameSpace": "attributes",
                                "class": "SVGTextAttributes"
                            }
                        },
                        "extra": {
                            "topRy": {
                                "class": "string | number"
                            }
                        }
                    }
                },
                "ellipse": {
                    "properties": {
                        "attrs": {
                            "body": {
                                "nameSpace": "attributes",
                                "class": "SVGEllipseAttributes"
                            },
                            "label": {
                                "nameSpace": "attributes",
                                "class": "SVGTextAttributes"
                            }
                        }
                    }
                },
                "embedded-image": {
                    "properties": {
                        "attrs": {
                            "body": {
                                "nameSpace": "attributes",
                                "class": "SVGRectAttributes"
                            },
                            "image": {
                                "nameSpace": "attributes",
                                "class": "SVGImageAttributes"
                            },
                            "label": {
                                "nameSpace": "attributes",
                                "class": "SVGTextAttributes"
                            }
                        }
                    }
                },
                "headered-rectangle": {
                    "properties": {
                        "attrs": {
                            "body": {
                                "nameSpace": "attributes",
                                "class": "SVGRectAttributes"
                            },
                            "header": {
                                "nameSpace": "attributes",
                                "class": "SVGRectAttributes"
                            },
                            "headerText": {
                                "nameSpace": "attributes",
                                "class": "SVGTextAttributes"
                            },
                            "bodyText": {
                                "nameSpace": "attributes",
                                "class": "SVGTextAttributes"
                            }
                        }
                    }
                },
                "image": {
                    "properties": {
                        "attrs": {
                            "image": {
                                "nameSpace": "attributes",
                                "class": "SVGImageAttributes"
                            },
                            "label": {
                                "nameSpace": "attributes",
                                "class": "SVGTextAttributes"
                            }
                        }
                    }
                },
                "inscribed-image": {
                    "properties": {
                        "attrs": {
                            "image": {
                                "nameSpace": "attributes",
                                "class": "SVGImageAttributes"
                            },
                            "border": {
                                "nameSpace": "attributes",
                                "class": "SVGEllipseAttributes"
                            },
                            "background": {
                                "nameSpace": "attributes",
                                "class": "SVGEllipseAttributes"
                            },
                            "label": {
                                "nameSpace": "attributes",
                                "class": "SVGTextAttributes"
                            }
                        }
                    }
                },
                "path": {
                    "properties": {
                        "attrs": {
                            "body": {
                                "nameSpace": "attributes",
                                "class": "SVGPathAttributes"
                            },
                            "label": {
                                "nameSpace": "attributes",
                                "class": "SVGTextAttributes"
                            }
                        }
                    }
                },
                "polygon": {
                    "properties": {
                        "attrs": {
                            "body": {
                                "nameSpace": "attributes",
                                "class": "SVGPolygonAttributes"
                            },
                            "label": {
                                "nameSpace": "attributes",
                                "class": "SVGTextAttributes"
                            }
                        }
                    }
                },
                "polyline": {
                    "properties": {
                        "attrs": {
                            "body": {
                                "nameSpace": "attributes",
                                "class": "SVGPolylineAttributes"
                            },
                            "label": {
                                "nameSpace": "attributes",
                                "class": "SVGTextAttributes"
                            }
                        }
                    }
                },
                "rectangle": {
                    "properties": {
                        "attrs": {
                            "body": {
                                "nameSpace": "attributes",
                                "class": "SVGRectAttributes"
                            },
                            "label": {
                                "nameSpace": "attributes",
                                "class": "SVGTextAttributes"
                            }
                        }
                    }
                }
            },
            "links": {
                "double-link": {
                    "properties": {
                        "attrs": {
                            "outline": {
                                "nameSpace": "attributes",
                                "class": "SVGPathAttributes"
                            }
                        }
                    }
                },
                "link": {
                    "properties": {
                    }
                }
            }
        },
        "uml": {
            "defaults": {
                "elements": {
                    "shapeObjectClass": {
                        "nameSpace": "shapes",
                        "class": "uml.__name@classify__"
                    },
                    "shapeOptionsClass": {
                        "nameSpace": "shapes",
                        "class": "uml.ClassAttributes"
                    }
                },
                "links": {
                    "shapeObjectClass": {
                        "nameSpace": "shapes",
                        "class": "uml.__name@classify__"
                    },
                    "shapeOptionsClass": {
                        "nameSpace": "dia",
                        "class": "Link.Attributes"
                    }
                }
            },
            "elements": {
                "abstract": {
                    "properties": {
                    }
                },
                "class": {
                    "properties": {
                    }
                },
                "interface": {
                    "properties": {
                    }
                }
            },
            "links": {
                "aggregation": {
                    "properties": {
                    }
                },
                "composition": {
                    "properties": {
                    }
                },
                "generalization": {
                    "properties": {
                    }
                },
                "implementation": {
                    "properties": {
                    }
                }
            }
        },
        "angular": {
            "defaults": {
                "elements": {
                    "shapeObjectClass": {
                        "nameSpace": "shapes",
                        "class": "basic.Rect"
                    },
                    "shapeOptionsClass": {
                        "nameSpace": "dia",
                        "class": "Element.GenericAttributes",
                        "type": "shapes.basic.RectSelectors"
                    },
                    "ngElementRef": true
                },
                "links": {
                    "shapeObjectClass": {
                        "nameSpace": "shapes",
                        "class": "standard.Link"
                    },
                    "shapeOptionsClass": {
                        "nameSpace": "dia",
                        "class": "Link.GenericAttributes",
                        "type": "shapes.standard.LinkSelectors"
                    }
                }
            },
            "elements": {
                "element": {
                    "properties": {
                    },
                    "template": "    <div class=\"angular-element\">\r\n      <ng-content><\/ng-content>\r\n    <\/div>",
                    "style": "   .angular-element {\r\n     pointer-events: none;\r\n     -moz-user-select: none;\r\n     -ms-user-select: none;\r\n     user-select: none;\r\n     -webkit-user-select: none;\r\n     position: absolute;\r\n     z-index: 2\r\n   }"
                }
            },
            "links": {
            }
        }
    }
}