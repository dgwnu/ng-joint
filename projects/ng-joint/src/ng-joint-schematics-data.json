{
    "defaults": {
        "importMappings": [
            {
                "importSymbols": ["shapes", "attributes"],
                "fromPath": "jointjs"
            }
        ]
    },
    "shapes" : {
        "standard": {
            "elements": {
                "circle": {
                    "properties": {
                        "attrs": {
                            "body": {
                                "nameSpace": "attributes",
                                "class": "SVGPathAttributes"
                            }
                        }
                    }
                }
            },
            "links": {
                "double-link": {
                    "properties": {
                        "attrs": {
                            "outline": {
                                "nameSpace": "attributes",
                                "class": "SVGPathAttributes"
                            }
                        }
                    }
                }
            }
        }
    }
}