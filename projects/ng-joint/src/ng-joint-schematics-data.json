{
    "defaults": {
        "importMappings": [
            {
                "importSymbols": ["shapes", "attributes", "dia"],
                "fromPath": "jointjs"
            }
        ]
    },
    "shapes" : {
        "standard": {
            "defaults": {
                "elements": {
                    "shapeObjectClass": {
                        "nameSpace": "shapes",
                        "class": "standard.__name@classify__"
                    },
                    "shapeOptionsClass": {
                        "nameSpace": "dia",
                        "class": "Element.GenericAttributes",
                        "type": "shapes.standard.__name@classify__Selectors"
                    }
                },
                "links": {
                    "shapeObjectClass": {
                        "nameSpace": "shapes",
                        "class": "standard.__name@classify__"
                    },
                    "shapeOptionsClass": {
                        "nameSpace": "dia",
                        "class": "Link.GenericAttributes",
                        "type": "shapes.standard.__name@classify__Selectors"
                    }
                }
            },
            "elements": {
                "circle": {
                    "properties": {
                        "attrs": {
                            "body": {
                                "nameSpace": "attributes",
                                "class": "SVGPathAttributes"
                            }
                        }
                    }
                }
            },
            "links": {
                "double-link": {
                    "properties": {
                        "attrs": {
                            "outline": {
                                "nameSpace": "attributes",
                                "class": "SVGPathAttributes"
                            }
                        }
                    }
                }
            }
        }
    }
}